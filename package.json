{
  "name": "moto-csgo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "prettier-tslint fix",
      "git add"
    ],
    "src/**/*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.*css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/lodash": "^4.14.161",
    "lodash": "^4.17.20",
    "ng-tournament-tree": "^2.0.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@types/node": "^12.12.55",
    "codelyzer": "^6.0.0-next.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
